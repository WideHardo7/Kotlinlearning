                    android:layout_height="wrap_content"
                    android:layout_marginStart="23dp"
                    android:layout_marginLeft="23dp"
                    android:layout_marginTop="47dp"
                    android:text="x/5"
                    app:layout_constraintStart_toEndOf="@+id/ob_pb_classi"
