                    android:layout_width="match_parent"
                    android:layout_height="73dp"
                    android:background="@android:drawable/divider_horizontal_textfield"
                    android:gravity="center_vertical"
                    android:paddingLeft="23dp"
                    android:text="@string/Obbiettivi"
                    android:textAllCaps="true"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/ob_variabili"
                    android:layout_width="match_parent"
                    android:layout_height="73dp"
                    android:layout_marginTop="73dp"
                    android:gravity="center_vertical"
                    android:paddingLeft="23dp"
                    android:text="@string/b.variabili"
                    android:textAllCaps="true"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/ob_stringhe"
                    android:layout_width="match_parent"
                    android:layout_height="73dp"
                    android:layout_marginTop="146dp"
                    android:gravity="center_vertical"
                    android:paddingLeft="23dp"
                    android:text="@string/b.strighe"
                    android:textAllCaps="true"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/ob_cicli"
                    android:layout_width="match_parent"
                    android:layout_height="73dp"
                    android:layout_marginTop="219dp"
                    android:gravity="center_vertical"
                    android:paddingLeft="23dp"
                    android:text="@string/b.cicli"
                    android:textAllCaps="true"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/ob_funzioni"
                    android:layout_width="match_parent"
                    android:layout_height="73dp"
                    android:layout_marginTop="292dp"
                    android:gravity="center_vertical"
                    android:paddingLeft="23dp"
                    android:text="@string/b.funzioni"
                    android:textAllCaps="true"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/ob_null"
                    android:layout_width="match_parent"
                    android:layout_height="73dp"
                    android:layout_marginTop="365dp"
                    android:gravity="center_vertical"
                    android:paddingLeft="23dp"
                    android:text="@string/b.null_safety"
                    android:textAllCaps="true"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
