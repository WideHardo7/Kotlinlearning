                    android:layout_height="73dp"
                    android:layout_marginTop="657dp"
                    android:gravity="center_vertical"
                    android:paddingLeft="23dp"
                    android:text="@string/b.lambda"
                    android:textAllCaps="true"
                    app:layout_constraintTop_toTopOf="parent" />


                <ProgressBar
                    android:id="@+id/ob_pb_variabili"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="150dp"
                    android:progress="0"
                    android:indeterminate="false"
                    android:max="5"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="23dp"
                    android:layout_marginLeft="23dp"
                    android:layout_marginTop="51dp"
                    app:layout_constraintStart_toStartOf="@+id/ob_variabili"
                    app:layout_constraintTop_toBottomOf="@+id/Achievements" />

                <ProgressBar
                    android:id="@+id/ob_pb_stringhe"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="150dp"
