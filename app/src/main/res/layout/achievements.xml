                    android:layout_marginLeft="23dp"
                    android:layout_marginTop="51dp"

                    app:layout_constraintStart_toStartOf="@+id/ob_ereditarietà"
                    app:layout_constraintTop_toBottomOf="@+id/ob_classi" />

                <ProgressBar
                    android:id="@+id/ob_pb_lambda"
                    style="?android:attr/progressBarStyleHorizontal"
                    android:layout_width="150dp"
                    android:progress="0"
                    android:indeterminate="false"
                    android:max="5"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="23dp"
                    android:layout_marginLeft="23dp"
                    android:layout_marginTop="51dp"
                    app:layout_constraintStart_toStartOf="@+id/ob_lambda"
                    app:layout_constraintTop_toBottomOf="@+id/ob_ereditarietà" />

                <TextView
                    android:id="@+id/ob_txpb_variabili"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="23dp"
                    android:layout_marginLeft="23dp"
                    android:layout_marginTop="47dp"
                    android:text="x/5"
                    app:layout_constraintStart_toEndOf="@+id/ob_pb_variabili"
                    app:layout_constraintTop_toBottomOf="@+id/Achievements" />

                <TextView
                    android:id="@+id/ob_txpb_stringhe"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="23dp"
                    android:layout_marginLeft="23dp"
                    android:layout_marginTop="47dp"
                    android:text="x/5"
                    app:layout_constraintStart_toEndOf="@+id/ob_pb_stringhe"
